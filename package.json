{
  "name": "mark-masons-ro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run generate:spec && npm run generate:api && ng build --prod --localize",
    "build:default": "ng build --configuration=production",
    "build:ro": "ng build --configuration=production,ro",
    "generate:spec": "swagger-cli bundle spec/api.yaml -o mark-api.yaml -t yaml",
    "generate:api": "openapi-generator generate -g typescript-angular -i mark-api.yaml -o ./src/app/generated",
    "sqless:start": "sqless start",
    "sqless:update": "sqless update -a mark-api.yaml",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.5",
    "@angular/localize": "^10.0.5",
    "@angular/material": "^10.1.0",
    "@angular/material-moment-adapter": "10.0.2",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@fullcalendar/angular": "5.1.0",
    "@fullcalendar/daygrid": "5.1.0",
    "@fullcalendar/interaction": "5.1.0",
    "@fullcalendar/list": "5.1.0",
    "@fullcalendar/moment": "5.1.0",
    "@types/faker": "^4.1.12",
    "@types/file-saver": "^2.0.1",
    "angular-oauth2-oidc": "^10.0.3",
    "docxtemplater": "^3.19.6",
    "faker": "^4.1.0",
    "file-saver": "^2.0.2",
    "moment": "^2.27.0",
    "ngx-quill": "^12.0.1",
    "marked": "^1.2.5",
    "pizzip": "^3.0.6",
    "quill": "^1.3.7",
    "papaparse": "5.3.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@openapitools/openapi-generator-cli": "1.0.18-4.3.1",
    "@apidevtools/swagger-cli": "4.0.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^1.2.0",
    "@types/papaparse": "4.1.34",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
